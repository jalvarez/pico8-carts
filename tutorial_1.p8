__lua__
function _init()
 t = 0
 s = 1
	x = 64
	y = 64
end

function _update()
 t = t + 1
 --[[
 if (t%8 < 4) then
 	s = 1
 else
  s = 2
 end
 ]]--
 s = 1
	if (btn(0)) then x=x-1 s=3 end
	if (btn(1)) then x=x+1 s=2 end
	if (btn(2)) then y=y-1 end
	if (btn(3)) then y=y+1 end
end

function _draw()
	rectfill(0,0,127,127,5)
	spr(s,x,y)
end
